{
  "expo": {
    "name": "movie-demo",
    "slug": "movie-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon-round.png",
    "splash": {
      "image": "./assets/app-icon-round.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.movie-demo.movie-demo",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon-round.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/app-icon-round.png"
    }
  }
}
